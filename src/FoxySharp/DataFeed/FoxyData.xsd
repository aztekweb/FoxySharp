  <xs:element name="shipping_address1" type="xs:string"/>
  <xs:element name="shipping_address2" type="xs:string"/>
  <xs:element name="shipping_city" type="xs:string"/>
  <xs:element name="shipping_state" type="xs:string"/>
  <xs:element name="shipping_postal_code" type="xs:string"/>
  <xs:element name="shipping_country" type="xs:string"/>
  <xs:element name="shipping_phone" type="xs:string"/>
