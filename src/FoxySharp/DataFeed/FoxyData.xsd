        <xs:element ref="product_total"/>
        <xs:element ref="tax_total"/>
        <xs:element ref="shipping_total"/>
        <xs:element ref="order_total"/>
        <xs:element ref="payment_gateway_type"/>
        <xs:element ref="receipt_url"/>
        <xs:element ref="taxes"/>
        <xs:element ref="discounts"/>
        <xs:element ref="attributes"/>
        <xs:element ref="status"/>
        <xs:element ref="customer_password"/>
        <xs:element ref="customer_password_salt"/>
        <xs:element ref="customer_password_hash_type"/>
        <xs:element ref="customer_password_hash_config"/>
        <xs:element ref="custom_fields"/>
        <xs:element ref="transaction_details"/>
        <xs:element ref="shipto_addresses"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="transactions">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="transaction" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="foxydata">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="transactions"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>