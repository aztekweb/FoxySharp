  <xs:element name="cc_number_masked" type="xs:string"/>
  <xs:element name="cc_type" type="xs:string"/>
  <xs:element name="cc_exp_month" type="xs:byte"/>
  <xs:element name="cc_exp_year" type="xs:short"/>
  <xs:element name="cc_start_date_month" type="xs:byte"/>
  <xs:element name="cc_start_date_year" type="xs:short"/>
  <xs:element name="cc_issue_number" type="xs:byte"/>
  <xs:element name="product_total" type="xs:float"/>
  <xs:element name="tax_total" type="xs:byte"/>
  <xs:element name="shipping_total" type="xs:float"/>
  <xs:element name="order_total" type="xs:float"/>
  <xs:element name="payment_gateway_type" type="xs:string"/>
  <xs:element name="receipt_url" type="xs:anyURI"/>
  <xs:element name="taxes" type="xs:string"/>
  <xs:element name="discounts" type="xs:string"/>
  <xs:element name="attributes" type="xs:string"/>
  <xs:element name="status" type="xs:string"/>
  <xs:element name="customer_password" type="xs:string"/>
  <xs:element name="customer_password_salt" type="xs:string"/>
  <xs:element name="customer_password_hash_type" type="xs:string"/>
  <xs:element name="customer_password_hash_config" type="xs:byte"/>
  <xs:element name="custom_fields">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="custom_field" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="transaction_details">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="transaction_detail" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="shipto_addresses" type="xs:string"/>
