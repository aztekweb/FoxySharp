<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="custom_field_name" type="xs:string"/>
  <xs:element name="custom_field_value" type="xs:string"/>
  <xs:element name="custom_field_is_hidden" type="xs:byte"/>
  <xs:element name="custom_field">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="custom_field_name"/>
        <xs:element ref="custom_field_value"/>
        <xs:element ref="custom_field_is_hidden"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="product_option_name" type="xs:string"/>
  <xs:element name="product_option_value" type="xs:string"/>
  <xs:element name="price_mod" type="xs:float"/>
  <xs:element name="weight_mod" type="xs:float"/>
  <xs:element name="transaction_detail_option">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="product_option_name"/>
        <xs:element ref="product_option_value"/>
        <xs:element ref="price_mod"/>
        <xs:element ref="weight_mod"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="product_name" type="xs:string"/>
  <xs:element name="product_price" type="xs:float"/>
  <xs:element name="product_quantity" type="xs:byte"/>
  <xs:element name="product_weight" type="xs:float"/>
  <xs:element name="product_code" type="xs:string"/>
  <xs:element name="parent_code" type="xs:string"/>
  <xs:element name="image" type="xs:string"/>
  <xs:element name="url" type="xs:string"/>
  <xs:element name="length" type="xs:string"/>
  <xs:element name="width" type="xs:string"/>
  <xs:element name="height" type="xs:string"/>
  <xs:element name="expires" type="xs:string"/>
  <xs:element name="sub_token_url" type="xs:string"/>
  <xs:element name="subscription_frequency" type="xs:string"/>
  <xs:element name="subscription_startdate" type="xs:string"/>
  <xs:element name="subscription_nextdate" type="xs:string"/>
  <xs:element name="subscription_enddate" type="xs:string"/>
  <xs:element name="is_future_line_item" type="xs:byte"/>
  <xs:element name="shipto" type="xs:string"/>
  <xs:element name="category_description" type="xs:string"/>
  <xs:element name="category_code" type="xs:string"/>
  <xs:element name="product_delivery_type" type="xs:string"/>
  <xs:element name="transaction_detail_options">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="transaction_detail_option" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="transaction_detail">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="product_name"/>
        <xs:element ref="product_price"/>
        <xs:element ref="product_quantity"/>
        <xs:element ref="product_weight"/>
        <xs:element ref="product_code"/>
        <xs:element ref="parent_code"/>
        <xs:element ref="image"/>
        <xs:element ref="url"/>
        <xs:element ref="length"/>
        <xs:element ref="width"/>
        <xs:element ref="height"/>
        <xs:element ref="expires"/>
        <xs:element ref="sub_token_url"/>
        <xs:element ref="subscription_frequency"/>
        <xs:element ref="subscription_startdate"/>
        <xs:element ref="subscription_nextdate"/>
        <xs:element ref="subscription_enddate"/>
        <xs:element ref="is_future_line_item"/>
        <xs:element ref="shipto"/>
        <xs:element ref="category_description"/>
        <xs:element ref="category_code"/>
        <xs:element ref="product_delivery_type"/>
        <xs:element ref="transaction_detail_options"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="id" type="xs:int"/>
  <xs:element name="store_id" type="xs:byte"/>
  <xs:element name="store_version" type="xs:string"/>
  <xs:element name="is_test" type="xs:byte"/>
  <xs:element name="is_hidden" type="xs:byte"/>
  <xs:element name="data_is_fed" type="xs:byte"/>
